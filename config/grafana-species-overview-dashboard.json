                  { "options": { "uncommon": { "color": "yellow", "index": 0, "text": "Ongewoon" } }, "type": "value" },
                  { "options": { "rare": { "color": "orange", "index": 1, "text": "Zeldzaam" } }, "type": "value" },
                  { "options": { "very_rare": { "color": "red", "index": 2, "text": "Zeer Zeldzaam" } }, "type": "value" }
