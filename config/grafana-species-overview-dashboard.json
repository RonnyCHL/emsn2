          "rawSql": "WITH first_detections AS (SELECT common_name, MIN(detection_timestamp) as first_seen FROM bird_detections GROUP BY common_name) SELECT fd.common_name as \"Soort\", fd.first_seen as \"Eerste Detectie\", COUNT(*) as \"Totaal Detecties\" FROM first_detections fd JOIN bird_detections bd ON fd.common_name = bd.common_name WHERE fd.first_seen >= date_trunc('month', CURRENT_DATE) AND ($station = '$__all' OR bd.station IN ($station)) GROUP BY fd.common_name, fd.first_seen ORDER BY fd.first_seen DESC LIMIT 10",
