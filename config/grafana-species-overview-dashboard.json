          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT common_name as \"Soort\", rarity_tier as \"Rarity\", MAX(detection_timestamp) as \"Laatste Detectie\", COUNT(*) as \"Detecties\" FROM bird_detections WHERE detection_timestamp >= CURRENT_DATE - INTERVAL '7 days' AND rarity_tier IS NOT NULL AND rarity_tier != 'common' AND ($station = '$__all' OR station IN ($station)) GROUP BY common_name, rarity_tier ORDER BY CASE rarity_tier WHEN 'very_rare' THEN 1 WHEN 'rare' THEN 2 WHEN 'uncommon' THEN 3 ELSE 4 END, \"Detecties\" DESC LIMIT 15",
          "refId": "A"
        }]
