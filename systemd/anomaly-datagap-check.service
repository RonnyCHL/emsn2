[Unit]
Description=EMSN Data Gap Anomaly Check
After=network.target postgresql.service

[Service]
Type=oneshot
User=ronny
WorkingDirectory=/home/ronny/emsn2
ExecStart=/usr/bin/python3 /home/ronny/emsn2/scripts/anomaly/data_gap_checker.py
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target
