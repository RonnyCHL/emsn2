        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT * FROM (\n  SELECT \n    'bird_detections' as \"Table\",\n    COUNT(*) as \"Total Records\",\n    COUNT(*) FILTER (WHERE detection_timestamp >= NOW() - INTERVAL '24 hours') as \"Last 24h\",\n    MAX(detection_timestamp) as \"Latest Record\",\n    MIN(detection_timestamp) as \"Oldest Record\"\n  FROM bird_detections\n  UNION ALL\n  SELECT \n    'weather_data',\n    COUNT(*),\n    COUNT(*) FILTER (WHERE measurement_timestamp >= NOW() - INTERVAL '24 hours'),\n    MAX(measurement_timestamp),\n    MIN(measurement_timestamp)\n  FROM weather_data\n  UNION ALL\n  SELECT \n    'anomalies',\n    COUNT(*),\n    COUNT(*) FILTER (WHERE timestamp >= NOW() - INTERVAL '24 hours'),\n    MAX(timestamp),\n    MIN(timestamp)\n  FROM anomalies\n  UNION ALL\n  SELECT \n    'system_health',\n    COUNT(*),\n    COUNT(*) FILTER (WHERE measurement_timestamp >= NOW() - INTERVAL '24 hours'),\n    MAX(measurement_timestamp),\n    MIN(measurement_timestamp)\n  FROM system_health\n) AS freshness\nORDER BY \"Latest Record\" DESC",
          "refId": "A",
